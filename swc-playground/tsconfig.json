{
  "compilerOptions": {
    "outDir": "./dist",
    "lib": ["es2019"],
    "module": "commonjs",
    "target": "es2019",
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "isolatedModules": false
  },
  "include": ["src"],
  "exclude": ["src/main.ts", "src/mainAOT.ts", "src/proxyHelper.ts"]
}
