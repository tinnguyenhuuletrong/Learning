{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/libs/rs-wasm-bind/src",
  "targets": {
    "test": {
      "executor": "@nxrs/cargo:test",
      "options": {}
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "wasm-pack build",
          "mv -f ./pkg/ ../../_artifacts/assets/rs-wasm/"
        ],
        "parallel": false,
        "cwd": "packages/libs/rs-wasm-bind",
        "outputPath": [
          "packages/_artifacts/assets/rs-wasm"
        ]
      }
    },
    "lint": {
      "executor": "@nxrs/cargo:clippy",
      "options": {
        "fix": false,
        "failOnWarnings": true,
        "noDeps": true
      }
    }
  },
  "tags": []
}