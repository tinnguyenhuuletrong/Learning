{
  "name": "app-cli-play",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/app/cli-play/src",
  "projectType": "application",
  "implicitDependencies": ["rs_node_bind", "_artifacts"],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/app/cli-play",
        "main": "packages/app/cli-play/src/main.ts",
        "tsConfig": "packages/app/cli-play/tsconfig.app.json",
        "assets": ["packages/app/cli-play/src/assets"],
        "webpackConfig": "packages/app/cli-play/webpack.config.js",
        "compiler": "tsc",
        "target": "node"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/app/cli-play/src/environments/environment.ts",
              "with": "packages/app/cli-play/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "dependsOn": ["^build"],
      "options": {
        "buildTarget": "app-cli-play:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/app/cli-play/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/app/cli-play"],
      "options": {
        "jestConfig": "packages/app/cli-play/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
