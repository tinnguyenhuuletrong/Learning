{
  "name": "_artifacts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/_artifacts/assets",
  "projectType": "library",
  "implicitDependencies": ["rs_node_bind", "rs_wasm_bind"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx run rs_node_bind:build:production && cp target/release/librs_node_bind.dylib packages/_artifacts/assets/librs_node_bind.node",
          "npx nx run rs_wasm_bind:build"
        ],
        "parallel": true
      },
      "outputs": ["{workspaceRoot}/packages/_artifacts/assets"]
    }
  }
}
